{
  "name": "back",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./src/app.js",
    "dev": "NODE_ENV=development nodemon --watch src src/app.js",
    "win-dev": "set NODE_ENV=development nodemon --watch src src/app.js"
  },
  "dependencies": {
    "argon2": "^0.27.1",
    "celebrate": "^13.0.4",
    "cookie-parser": "~1.4.4",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-jwt": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.17",
    "mongoose-unique-validator": "^2.0.3",
    "normalize-port": "^1.0.0",
    "openapi-types": "^7.2.3",
    "swagger-jsdoc": "^5.0.1",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "eslint": "^7.21.0",
    "nodemon": "^2.0.7"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modueles",
      "*.test.js",
      "fixtures/*",
      "migrations/*"
    ],
    "delay": 1000,
    "execMap": {
      "rb": "ruby",
      "pde": "processing --sketch={{pwd}} --run"
    }
  }
}
