{
  "name": "back",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./src/app.js",
    "dev": "NODE_ENV=development nodemon --watch src src/app.js",
    "win-dev": "set NODE_ENV=development nodemon --watch src src/app.js",
    "db:migration": "npx sequelize db:migrate",
    "db:seed": "npx sequelize db:seed:all",
    "db:migration:create": "npx makemigration --name"
  },
  "dependencies": {
    "argon2": "^0.27.1",
    "axios": "^0.21.1",
    "celebrate": "^13.0.4",
    "cheerio": "^1.0.0-rc.5",
    "cookie-parser": "~1.4.4",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-jwt": "^6.0.0",
    "express-useragent": "^1.0.15",
    "jsonwebtoken": "^8.5.1",
    "normalize-port": "^1.0.0",
    "openapi-types": "^7.2.3",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "request-ip": "^2.1.3",
    "sequelize": "^6.5.0",
    "sequelize-auto-migrations-v2": "^1.1.0",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "eslint": "^7.21.0",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0"
  },
  "nodemonConfig": {
    "ignore": [
      ".idea",
      "node_modueles",
      "*.test.js",
      "fixtures/*",
      "migrations/*"
    ],
    "delay": 1000,
    "execMap": {
      "rb": "ruby",
      "pde": "processing --sketch={{pwd}} --run"
    }
  }
}
